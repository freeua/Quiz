{
  "name": "sanofi-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "axios-extensions": "^3.0.6",
    "core-js": "^3.6.4",
    "formik": "^2.0.4",
    "localforage": "^1.7.3",
    "rc-select": "^10.0.0-alpha.29",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "yup": "^0.27.0"
